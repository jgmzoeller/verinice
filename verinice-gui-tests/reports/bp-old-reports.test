proc "gen-rep" [val report] {
	try {
		start-time-measure $report
		gen-rep-com $report Grundschutzmodell "importierte Objekte/  RECPLAST [SerNet]"
		stop-time-measure $report | write-lines [concat "file:" $basedir "/report-measure.csv"] true
	} -catch {
		log [concat "ERROR creating report " $report]
		try {
			get-window "Fehler" | get-button OK | click
		} -catch {}
	}
}

log "Start report testing old bp"
enable-reporting-log

import-vna-bp-old "RECPLAST.vna"
wait-for-import "Grundschutzmodell" "importierte Objekte/  RECPLAST [SerNet]"

gen-rep "(S) BSI IT-Grundschutz: A.1 Strukturanalyse"
gen-rep "(S) BSI IT-Grundschutz: A.1 Strukturanalyse-Abhängigkeiten"
gen-rep "(S) BSI IT-Grundschutz: A.2 Schutzbedarfsanalyse"
gen-rep "(S) BSI IT-Grundschutz: A.3 Modellierung"
gen-rep "(S) BSI IT-Grundschutz: A.4 Basis-Sicherheits-Check"
gen-rep "(S) BSI IT-Grundschutz: A.5 Ergänzende Sicherheitsanalyse"
gen-rep "(S) BSI IT-Grundschutz: A.6 Risikoanalyse"
gen-rep "(S) BSI IT-Grundschutz: A.7 Managementbewertung über bestehende Risiken"
gen-rep "(S) BSI IT-Grundschutz: Auditbericht"
gen-rep "(S) BSI IT-Grundschutz: Realisierungsplan"
gen-rep "(S) BSI IT-Grundschutz: Umsetzungsstatus (graphisch)"
gen-rep "(S) BSI 100-4: Gesamtüberblick Schadensbewertung"
gen-rep "(S) BSI 100-4: Gesamtüberblick Schadensbewertung mit Summenbildung"
gen-rep "(S) BSI 100-4: Kosten Nutzen Analyse"


delete-bp-old-element "importierte Objekte/  RECPLAST [SerNet]"


